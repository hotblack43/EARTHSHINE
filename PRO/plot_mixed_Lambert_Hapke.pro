im1=readfits('NinetyTenMixed.fits')
im2=readfits('TenNinetyMixed.fits')
im0=readfits('FiftyFiftyMixed.fits')
;
diff1=(im1-im0)/im0*100.0
writefits,'NinetyTenDiff.fits',diff1
diff2=(im2-im0)/im0*100.0
writefits,'TenNinetyDiff.fits',diff2
;
levs=[-100,-90,-80,-70,-60,-50,-45,-40,-30,-20,-10,0,10,20,30,40,50]
contour,diff1,/isotropic,ystyle=3,xstyle=3,/cell_fill,nlevels=101
contour,diff1,/overplot,/isotropic,ystyle=3,xstyle=3,levels=levs,c_labels=findgen(100)*0+1,c_charsize=1.6
end
