im=readfits('BminusVimage.fits')
im=smooth(im,21)
device,decomposed=0
loadct,13
levs=(findgen(21)/20.)/0.5
levs=[0.1,levs,1.01,1.02,1.03,1.04,1.05,1.06,1.07,1.08,1.09]
levs=levs(sort(levs))
levs=levs(uniq(levs))
print,levs
contour,/isotropic,xstyle=3,ystyle=3,im,/cell_fill,levels=levs
contour,c_labels=findgen(20)*0+1,color=fsc_color('white'),/isotropic,/overplot,im,levels=levs
cursor,a,b
print,'DS:',mean(im(a-10:a+10,b-10:b+10))
wait,1
cursor,a,b
print,'BS:',mean(im(a-10:a+10,b-10:b+10))
end
